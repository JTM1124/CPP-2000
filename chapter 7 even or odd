This program will check if a number is even or odd*/


#include <iostream>
using namespace std; 

int main()
{
   
	int number;


	cout << "Enter a number: " << endl; 
	cin >> number; // takes in the number 

	if (number % 2 == 0)
	{
		cout << "Even" << endl; 
	}
	else
	{
		cout << "Odd" << endl; 
	}
	return 0; 









}
